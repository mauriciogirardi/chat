{
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": ["http://localhost:3000"],
        "method": ["GET", "POST", "PUT", "DELETE"],
        "maxAgeSeconds": 3600,
        "responseHeader": ["Content-Type", "Access-Control-Allow-Origin", "Access-Control-Allow-Methods", "Access-Control-Allow-Headers", "Access-Control-Max-Age"]
      }
    ]
  }
}

